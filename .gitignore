# ignore anything generated by the OS
thumbs.db
@eaDir
.thumbs
.DS_Store

# All caches
cache/
.yarn/

# Any node_modules, in any folder
node_modules

# Any log files, in any folder
*.log
logs
log.txt

# Ignore everything in the root except the "wp-content" directory.
/*

# don't ignore any gitignores
!.gitignore

# don't ignore readme
!README.md

# don't ignore wp-content
!wp-content/

# Example themes
wp-content/themes/twenty*/

# Ignore everything inside of wp-content
wp-content/*

# Example plugin
/wp-content/plugins/hello.php

# Stupid indexes
/wp-content/plugins/index.php
/wp-content/themes/index.php

# Don't ignore the rest of the plugins
!wp-content/plugins/

# Don't ignore the rest of the themes
!wp-content/themes/

# so that sql files litespeed needs aren't missed
!wp-content/plugins/litespeed/*

# php vendor directories
/vendor
/wp-content/themes/*/vendor

# for google sitekit to not break
!wp-content/plugins/google-site-kit/third-party/psr/cache/